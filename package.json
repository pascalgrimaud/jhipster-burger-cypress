{
  "name": "burger-cypress",
  "version": "0.0.0",
  "private": true,
  "description": "Description for burgerCypress",
  "license": "UNLICENSED",
  "scripts": {
    "backend:doc:test": "./mvnw -ntp javadoc:javadoc --batch-mode",
    "backend:info": "./mvnw -ntp enforcer:display-info --batch-mode",
    "backend:nohttp:test": "./mvnw -ntp checkstyle:check --batch-mode",
    "backend:unit:test": "./mvnw -ntp -P-webpack verify --batch-mode -Dlogging.level.ROOT=OFF -Dlogging.level.org.zalando=OFF -Dlogging.level.tech.jhipster=OFF -Dlogging.level.io.github.pascalgrimaud=OFF -Dlogging.level.org.springframework=OFF -Dlogging.level.org.springframework.web=OFF -Dlogging.level.org.springframework.security=OFF",
    "build": "npm run webpack:prod",
    "ci:backend:test": "npm run backend:info && npm run backend:doc:test && npm run backend:nohttp:test && npm run backend:unit:test",
    "ci:e2e:package": "npm run java:$npm_package_config_packaging:$npm_package_config_default_environment -- -Pe2e -Denforcer.skip=true",
    "ci:e2e:prepare": "npm run ci:e2e:prepare:docker",
    "ci:e2e:prepare:docker": "npm run docker:db:up && npm run docker:others:up && docker ps -a",
    "ci:e2e:run": "concurrently -k -s first \"npm run ci:e2e:server:start\" \"npm run e2e:headless\"",
    "preci:e2e:server:start": "npm run docker:db:await --if-present && npm run docker:others:await --if-present",
    "ci:e2e:server:start": "java -jar target/e2e.$npm_package_config_packaging --spring.profiles.active=$npm_package_config_default_environment -Dlogging.level.ROOT=OFF -Dlogging.level.org.zalando=OFF -Dlogging.level.tech.jhipster=OFF -Dlogging.level.io.github.pascalgrimaud=OFF --logging.level.org.springframework.web=ERROR",
    "ci:e2e:teardown": "npm run ci:e2e:teardown:docker",
    "ci:e2e:teardown:docker": "npm run docker:db:down --if-present && npm run docker:others:down && docker ps -a",
    "ci:frontend:build": "npm run webpack:build:$npm_package_config_default_environment",
    "ci:frontend:test": "npm run ci:frontend:build && npm test",
    "ci:server:await": "echo \"Waiting for server at port $npm_package_config_backend_port to start\" && wait-on http-get://localhost:$npm_package_config_backend_port/management/health && echo \"Server at port $npm_package_config_backend_port started\"",
    "ci:server:package": "npm run java:$npm_package_config_packaging:$npm_package_config_default_environment",
    "clean-www": "rimraf target/classes/static/app/{src,target/}",
    "cleanup": "rimraf target/classes/static/",
    "cypress": "cypress open",
    "docker:db:down": "docker-compose -f src/main/docker/postgresql.yml down -v --remove-orphans",
    "docker:db:up": "docker-compose -f src/main/docker/postgresql.yml up -d",
    "docker:others:down": "",
    "predocker:others:up": "",
    "docker:others:up": "",
    "e2e": "npm run e2e:cypress",
    "e2e:cypress": "cypress run --browser chrome",
    "e2e:cypress:headless": "npm run e2e:cypress -- --headless",
    "e2e:dev": "concurrently -k -s first \"./mvnw\" \"e2e:run\"",
    "pree2e:headless": "npm run ci:server:await",
    "e2e:headless": "npm run e2e:cypress:headless",
    "java:docker": "./mvnw -ntp verify -DskipTests jib:dockerBuild",
    "java:docker:dev": "npm run java:docker -- -Pdev,webpack",
    "java:docker:prod": "npm run java:docker -- -Pprod",
    "java:jar": "./mvnw -ntp verify -DskipTests --batch-mode",
    "java:jar:dev": "npm run java:jar -- -Pdev,webpack",
    "java:jar:prod": "npm run java:jar -- -Pprod",
    "java:war": "./mvnw -ntp verify -DskipTests --batch-mode -Pwar",
    "java:war:dev": "npm run java:war -- -Pdev,webpack",
    "java:war:prod": "npm run java:war -- -Pprod",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --no-cache --config src/test/javascript/jest.conf.js",
    "jest:update": "npm run jest -- --updateSnapshot",
    "lint": "tslint --project tsconfig.json -e 'node_modules/**'",
    "lint:fix": "npm run lint -- --fix",
    "prettier:format": "prettier --write \"{,src/**/,webpack/}*.{md,json,yml,js,ts,tsx,css,scss,vue,java}\"",
    "serve": "npm run start",
    "start": "npm run webpack:dev",
    "start-tls": "npm run webpack:dev -- --env.tls",
    "pretest": "npm run lint",
    "test": "npm run jest",
    "test-ci": "npm run lint && npm run jest:update",
    "test:watch": "npm run jest -- --watch",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack:build": "npm run cleanup && npm run webpack:build:dev",
    "webpack:build:dev": "npm run webpack -- --config webpack/webpack.dev.js --profile --env.stats=minimal",
    "webpack:build:prod": "npm run webpack -- --config webpack/webpack.prod.js --profile --env.stats=minimal",
    "webpack:dev": "npm run webpack-dev-server -- --config webpack/webpack.dev.js --progress --inline --env.stats=normal",
    "webpack:prod": "npm run cleanup && npm run webpack:build:prod && npm run clean-www",
    "webpack:test": "npm run test"
  },
  "config": {
    "backend_port": "8080",
    "default_environment": "prod",
    "packaging": "jar"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.30",
    "@fortawesome/free-solid-svg-icons": "5.14.0",
    "@fortawesome/vue-fontawesome": "0.1.10",
    "@openapitools/openapi-generator-cli": "1.0.13-4.3.1",
    "axios": "0.19.2",
    "bootstrap": "4.5.0",
    "bootstrap-vue": "2.14.0",
    "dayjs": "1.9.5",
    "swagger-ui-dist": "3.25.4",
    "vue": "2.6.12",
    "vue-class-component": "7.2.5",
    "vue-cookie": "1.1.4",
    "vue-i18n": "8.21.0",
    "vue-infinite-loading": "2.4.5",
    "vue-property-decorator": "9.0.0",
    "vue-router": "3.4.3",
    "vue2-filters": "0.11.0",
    "vuelidate": "0.7.5",
    "vuex": "3.5.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.10",
    "@types/node": "14.0.4",
    "@types/sinon": "9.0.3",
    "@types/vuelidate": "0.7.13",
    "@vue/cli-plugin-typescript": "4.5.4",
    "@vue/cli-service": "4.5.4",
    "@vue/test-utils": "1.0.4",
    "autoprefixer": "9.8.0",
    "babel-core": "7.0.0-bridge.0",
    "browser-sync": "2.26.7",
    "browser-sync-webpack-plugin": "2.2.2",
    "concurrently": "5.3.0",
    "copy-webpack-plugin": "6.0.1",
    "css-loader": "3.5.3",
    "cypress": "5.5.0",
    "file-loader": "6.0.0",
    "fork-ts-checker-webpack-plugin": "4.1.4",
    "friendly-errors-webpack-plugin": "1.7.0",
    "generator-jhipster": "6.10.4",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "jest": "26.4.2",
    "jest-junit": "11.1.0",
    "jest-serializer-vue": "2.0.2",
    "jest-sonar-reporter": "2.0.0",
    "jest-vue-preprocessor": "1.7.1",
    "lint-staged": "10.2.4",
    "merge-jsons-webpack-plugin": "1.0.21",
    "mini-css-extract-plugin": "0.9.0",
    "node-notifier": "7.0.0",
    "numeral": "2.0.6",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "portfinder": "1.0.26",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-url": "8.0.0",
    "prettier": "2.1.2",
    "prettier-plugin-java": "0.8.3",
    "rimraf": "3.0.2",
    "sass": "1.26.5",
    "sass-loader": "8.0.2",
    "sinon": "9.0.2",
    "terser-webpack-plugin": "3.0.1",
    "ts-jest": "26.2.0",
    "ts-loader": "7.0.4",
    "tslib": "2.0.0",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-loader": "3.6.0",
    "typescript": "4.0.5",
    "url-loader": "4.1.0",
    "vue-jest": "3.0.6",
    "vue-loader": "15.9.3",
    "vue-template-compiler": "2.6.12",
    "wait-on": "5.2.0",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.0.9"
  },
  "engines": {
    "node": ">=12.18.3",
    "npm": ">= 6.13.4"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "jestSonar": {
    "reportPath": "target/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  }
}
